{
	"compilerOptions": {
		"target": "ESNext",
		"useDefineForClassFields": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"strict": false,
		"jsx": "preserve",
		"sourceMap": true,
		"resolveJsonModule": true,
		"isolatedModules": true,
		"esModuleInterop": true,
		"lib": ["ESNext", "DOM"],
		"skipLibCheck": true
	},
	"include": [
		"../hhui/ui/**/src/**/*.ts",
		"../hhui/ui/**/src/**/*.d.ts",
		"../hhui/ui/**/src/**/*.tsx",
		"../hhui/ui/**/src/**/*.vue",
		"../hhui/build/**/*.vue",
		"../hhui/build/**/*.ts"
	]
}
