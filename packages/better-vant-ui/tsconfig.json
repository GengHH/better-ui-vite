{
	"target": "es5",
	"compilerOptions": {
		"declaration": true /* 生成相关的 '.d.ts' 文件。 */,
		"declarationDir": "./dist/types" /* '.d.ts' 文件输出目录 */,
		"jsx": "preserve"
	},
	"include": ["./**/*.*", "./src/global-vue.d.ts"],
	"exclude": ["node_modules"],
	"esModuleInterop": true,
	"allowSyntheticDefaultImports": "true",
	"lib": ["ESNext", "DOM", "es2016"]
}
